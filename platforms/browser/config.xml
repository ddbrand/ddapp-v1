<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ddrobotec.ddios" version="1.1.5" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DD App</name>
    <description>
        This app shows your newest trainingresults and let you log in on the machine with a QR code scanner.
    </description>
    <author email="mario@dynamicdevices.ch" href="https://ddrobotec.com">
        Dynamic Devices AG
    </author>
    <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
